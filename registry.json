{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "mikecebul",
  "homepage": "https://payload.mikecebul.com",
  "items": [
    {
      "name": "hello-world",
      "type": "registry:block",
      "title": "Hello World Payload Block",
      "description": "A Payload CMS block component with both component.tsx and config.ts files.",
      "dependencies": ["payload"],
      "registryDependencies": ["button", "utils"],
      "files": [
        {
          "path": "registry/new-york/blocks/hello-world/component.tsx",
          "type": "registry:file",
          "target": "blocks/HelloWorld/component.tsx"
        },
        {
          "path": "registry/new-york/blocks/hello-world/config.ts",
          "type": "registry:file",
          "target": "blocks/HelloWorld/config.ts"
        }
      ]
    },
    {
      "name": "payload-types",
      "type": "registry:lib",
      "title": "Payload Types",
      "description": "TypeScript type definitions for PayloadCMS blocks.",
      "files": [
        {
          "path": "registry/new-york/lib/types/payload-types.ts",
          "type": "registry:file",
          "target": "lib/types/payload-types.ts"
        }
      ]
    },
    {
      "name": "blocks-renderer",
      "type": "registry:component",
      "title": "Blocks Renderer",
      "description": "A component to render PayloadCMS blocks dynamically.",
      "registryDependencies": ["@payload/hello-world", "@payload/payload-types"],
      "files": [
        {
          "path": "registry/new-york/components/blocks-renderer.tsx",
          "type": "registry:file",
          "target": "components/blocks-renderer.tsx"
        }
      ]
    },
    {
      "name": "hello-world-demo",
      "type": "registry:page",
      "title": "Hello World Demo Page",
      "description": "A demo page showcasing PayloadCMS blocks with sample data.",
      "registryDependencies": ["@payload/blocks-renderer", "@payload/payload-types"],
      "files": [
        {
          "path": "registry/new-york/pages/hello-world-demo.tsx",
          "type": "registry:file",
          "target": "pages/hello-world-demo.tsx"
        }
      ]
    }
  ]
}